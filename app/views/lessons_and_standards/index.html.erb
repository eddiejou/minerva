<div class="page-header">
  <h1>
    Lessons and standards
    <a href="/lessons_and_standards/new" class="btn btn-lg btn-success">New Lessons and standard</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Lesson</th>
        <th>Standard</th>
        <th>Actions</th>
      </tr>

      <% @lessons_and_standards.each do |lessons_and_standard| %>
      <tr>
        <td><%= lessons_and_standard.lesson_id %></td>
        <td><%= lessons_and_standard.standard_id %></td>
        <td>
          <a href="/lessons_and_standards/<%= lessons_and_standard.id %>" class="btn btn-primary">Show</a>
          <a href="/lessons_and_standards/<%= lessons_and_standard.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_lessons_and_standard/<%= lessons_and_standard.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

<div class="page-header">
  <h1>
    Assignment likes
    <a href="/assignment_likes/new" class="btn btn-lg btn-success">New Assignment like</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Assignment</th>
        <th>Actions</th>
      </tr>

      <% @assignment_likes.each do |assignment_like| %>
      <tr>
        <td><%= assignment_like.user_id %></td>
        <td><%= assignment_like.assignment_id %></td>
        <td>
          <a href="/assignment_likes/<%= assignment_like.id %>" class="btn btn-primary">Show</a>
          <a href="/assignment_likes/<%= assignment_like.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_assignment_like/<%= assignment_like.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

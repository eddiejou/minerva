<div class="page-header">
  <h3>Lesson Like #<%= @lesson_like.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @lesson_like.user_id %></dd>

      <dt>Lesson</dt>
      <dd><%= @lesson_like.lesson_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/lesson_likes" class="btn btn-primary">
        Back
      </a>
      <a href="/lesson_likes/<%= @lesson_like.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_lesson_like/<%= @lesson_like.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>

<div class="page-header">
  <h1>
    Lesson likes
    <a href="/lesson_likes/new" class="btn btn-lg btn-success">New Lesson like</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Lesson</th>
        <th>Actions</th>
      </tr>

      <% @lesson_likes.each do |lesson_like| %>
      <tr>
        <td><%= lesson_like.user_id %></td>
        <td><%= lesson_like.lesson_id %></td>
        <td>
          <a href="/lesson_likes/<%= lesson_like.id %>" class="btn btn-primary">Show</a>
          <a href="/lesson_likes/<%= lesson_like.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_lesson_like/<%= lesson_like.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
